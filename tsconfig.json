{
  "compilerOptions": {
    "target": "ES6",
    "lib": ["es2016", "es2017", "es2018"],
    "module": "commonjs",
    "moduleResolution": "Node",
    "removeComments": true,
    "preserveConstEnums": true,
    "noImplicitAny": false,
    "inlineSourceMap": true,
    "declaration": true,
    "experimentalDecorators": true,
    "pretty": true
  },
  "exclude": [
    "node_modules",
    "cdk.out"
  ]
}